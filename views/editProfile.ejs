<%- include('./partials/links') %>
<body>
    <%- include('./partials/header') %>

    <h1 class="form-title"><%= title %></h1>
    <section class="form-section">
        
        <form action="/users/editprofile/storeEdit" method="POST">
            <div class="form-fields">
                <input type="text" placeholder="Nombre" value=<%= usuario.nombre %>>
                <input type="text" placeholder="Apellido" value=<%= usuario.apellido %>>
                <input type="date" placeholder="Fecha de nacimiento" value=<%= usuario.nacimiento %>>
                <input type="number" placeholder="DNI" value=<%= usuario.dni %>>
                <input type="number" placeholder="Celular" value=<%= usuario.celular %>>
                <input disabled type="email" placeholder="E-mail" value=<%= usuario.mail %> >
                <input type="password" placeholder="Contraseña" value=<%= usuario.contrasenia %>>
                <input type="file" name="Imagen" value="" >
                <!-- <input type="password" placeholder="Repetir Contraseña">   -->                 
                     
            </div>

            <button type="submit" class="form-button">
                Enviar
            </button>
            
        </form>
    </section>
    <%- include('./partials/footer') %>
</body>
</html>